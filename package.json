    "devDependencies": {
        "@types/ejs": "^3.1.1",
        "@types/glob": "^7.2.0",
        "@types/inquirer": "^8.2.1",
        "@types/node": "^17.0.34",
        "@types/shelljs": "^0.8.11",
        "@types/through": "^0.0.30",
        "typescript": "^4.6.4"
    },
